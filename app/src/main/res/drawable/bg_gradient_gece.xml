<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="135"
                android:centerColor="@color/gece_primary"
                android:endColor="@color/gece_primary_light"
                android:startColor="@color/gece_primary_dark"
                android:type="linear" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="135"
                android:endColor="#00FFFFFF"
                android:startColor="#05FFFFFF"
                android:type="linear" />
        </shape>
    </item>
</layer-list>
